<arquillian xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://jboss.org/schema/arquillian"
  xsi:schemaLocation="http://jboss.org/schema/arquillian http://jboss.org/schema/arquillian/arquillian_1_0.xsd">

  <extension qualifier="openshift">
    <property name="app.name">arquillian-cube-openshift-fmp-ftest</property>
    <property name="env.init.enabled">true</property>
    <!--<property name="fmp.build">true</property>-->
    <property name="fmp.maven.build">true</property>
    <property name="enableImageStreamDetection">false</property>
  </extension>

</arquillian>
